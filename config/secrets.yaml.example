# VELAS Trading System - Secrets Configuration
# Copy this file to secrets.yaml and fill in your credentials
# NEVER commit secrets.yaml to git!

# ===== Binance API =====
binance:
  api_key: "your_binance_api_key_here"
  api_secret: "your_binance_api_secret_here"
  
  # Optional: Testnet credentials
  testnet:
    enabled: false
    api_key: "your_testnet_api_key"
    api_secret: "your_testnet_api_secret"

# ===== Telegram =====
telegram:
  bot_token: "your_telegram_bot_token_here"
  
  # Channel for signals
  signal_channel_id: "-1001234567890"
  
  # Channel for alerts (can be same as signal)
  alert_channel_id: "-1001234567890"
  
  # Admin user ID for private alerts
  admin_user_id: "123456789"
